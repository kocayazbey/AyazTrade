  "shortcuts": [
    {
      "name": "Analytics",
      "short_name": "Analytics",
      "description": "View real-time analytics and reports",
      "url": "/analytics",
      "icons": [
        {
          "src": "/icons/analytics-icon.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Orders",
      "short_name": "Orders",
      "description": "Manage orders and fulfillment",
      "url": "/orders",
      "icons": [
        {
          "src": "/icons/orders-icon.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Products",
      "short_name": "Products",
      "description": "Manage product catalog",
      "url": "/products",
      "icons": [
        {
          "src": "/icons/products-icon.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Customers",
      "short_name": "Customers",
      "description": "View customer information",
      "url": "/customers",
      "icons": [
        {
          "src": "/icons/customers-icon.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "web",
      "url": "https://admin.ayaztrade.com"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred",
  "capture_links": "new-client",
  "file_handlers": [
    {
      "action": "/upload",
      "accept": {
        "text/csv": [".csv"],
        "application/vnd.ms-excel": [".xls"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+ayaztrade",
      "url": "/protocol-handler?protocol=%s"
    }
  ]
}
