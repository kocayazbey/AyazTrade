{
  "name": "ayaz-trade",
  "version": "1.0.0",
  "description": "AyazTrade - Enterprise Business Suite",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write src/**/*.ts test/**/*.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:unit": "jest --testPathPattern unit",
    "test:integration": "jest --testPathPattern integration",
    "test:e2e": "jest --testPathPattern e2e",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "ts-node scripts/seed-sample-data.ts",
    "verify": "ts-node scripts/verify-system.ts",
    "dev": "pnpm start:dev",
    "setup": "pnpm db:push && pnpm db:seed",
    "frontend:dev": "cd frontend/admin && pnpm dev",
    "frontend:build": "cd frontend/admin && pnpm build",
    "frontend:start": "cd frontend/admin && pnpm start"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.3.0",
