    "@nestjs/core": "^10.3.0",
    "@nestjs/elasticsearch": "^11.1.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^10.3.0",
    "@paralleldrive/cuid2": "^2.3.1",
    "@sentry/node": "^8.35.0",
    "@types/ioredis": "^4.28.10",
    "@types/joi": "^17.2.2",
    "@types/pg": "^8.15.5",
    "aws-sdk": "^2.1691.0",
    "bcrypt": "^5.1.1",
    "cache-manager": "^7.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "csv-parse": "^6.1.0",
    "dd-trace": "^5.21.0",
    "drizzle-kit": "^0.20.6",
    "drizzle-orm": "^0.29.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "joi": "^18.0.1",
    "newrelic": "^11.23.0",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "saml2-js": "^4.0.2",
    "socket.io": "^4.6.0",
    "speakeasy": "^2.0.0",
    "web-push": "^3.6.7"
