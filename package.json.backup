{
  "name": "ayaz-trade",
  "version": "1.0.0",
  "description": "AyazTrade - Enterprise Business Suite (AyazComm + AyazCRM + AyazERP + AyazWMS + AyazAnalytics)",
  "author": "Ayaz Technology",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node -r ./dist/otel.js dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:unit": "jest --testPathPattern=\"unit\"",
    "test:integration": "jest --testPathPattern=\"integration\"",
    "test:e2e": "jest --testPathPattern=\"e2e\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:comprehensive": "scripts/run-comprehensive-tests.bat",
    "test:decorators": "jest --testPathPattern=\"decorators\" --verbose",
    "test:interceptors": "jest --testPathPattern=\"interceptors\" --verbose",
    "test:integration": "jest --testPathPattern=\"comprehensive-integration\" --verbose",
    "test:performance": "jest --testPathPattern=\"performance\" --verbose",
    "load:test": "k6 run test/performance/load-test.js",
    "test:security": "jest --testPathPattern=\"security\" --verbose",
    "test:accessibility": "jest --testPathPattern=\"accessibility\" --verbose",
    "test:browser": "jest --testPathPattern=\"browser-compatibility\" --verbose",
    "test:mobile": "jest --testPathPattern=\"mobile-compatibility\" --verbose",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:sync": "npm run db:generate && npm run db:push",
    "db:seed": "ts-node scripts/seed-database.ts",
    "db:seed:dev": "NODE_ENV=development npm run db:seed",
    "schema:check-naming": "ts-node scripts/standardize-schema-naming.ts",
    "schema:fix-naming": "ts-node scripts/standardize-schema-naming.ts --fix",
    "dev": "concurrently \"npm run start:dev\" \"npm run dev:admin\" \"npm run dev:storefront\" \"npm run dev:b2b\"",
    "dev:admin": "cd frontend/admin && npm run dev",
    "dev:storefront": "cd frontend/storefront && npm run dev",
    "dev:b2b": "cd frontend/b2b-portal && npm run dev",
    "build:all": "npm run build && cd frontend/admin && npm run build && cd ../storefront && npm run build && cd ../b2b-portal && npm run build"
  },
